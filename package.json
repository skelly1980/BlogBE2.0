{
  "name": "blog-server",
  "version": "0.0.1",
  "keywords": [
    "Express",
    "TypeScript",
    "Starter"
  ],
  "author": "Shawn Kelly",
  "license": "ISC",
  "description": "Blogs Backend Server",
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "clean-install": "rm -rf ./node_modules && rm -f yarn.lock && yarn install",
    "dev-start": "NODE_ENV=dev ts-node ./src/server",
    "dev-start:hot": "nodemon --exec \"npm run dev-start\" --watch ./src --ext .ts",
    "lint": "eslint",
    "prettier": "npx prettier . --check",
    "test": "NODE_ENV=test && jest",
    "test:hot": "nodemon --exec \"npm run test\" --watch ./src --watch ./spec --ext .ts",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "~5.1.0",
    "helmet": "^8.0.0",
    "mongoose": "^8.17.1",
    "morgan": "~1.10.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.18.0",
    "@stylistic/eslint-plugin-js": "^4.4.0",
    "@stylistic/eslint-plugin-ts": "^4.4.0",
    "@types/cookie-parser": "^1.4.8",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.14",
    "@types/morgan": "^1.9.9",
    "@types/node": "^22.10.5",
    "@types/supertest": "^6.0.2",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-n": "^17.15.1",
    "eslint-plugin-prettier": "^5.2.3",
    "jest": "^29.7.0",
    "jiti": "^2.4.2",
    "nodemon": "^3.1.9",
    "prettier": "3.5.3",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.20.0"
  },
  "packageManager": "yarn@1.22.22+sha512.a6b2f7906b721bba3d67d4aff083df04dad64c399707841b7acf00f6b133b7ac24255f2652fa22ae3534329dc6180534e98d17432037ff6fd140556e2bb3137e"
}
